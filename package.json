	"scripts": {
		"start": "node tools/start.js",
		"build": "NODE_ENV=production webpack --config config/webpack.config.prod.js --hide-modules --display-optimization-bailout",
		"build:serve": "npm run build && npm run serve",
		"analyze": "NODE_ENV=production webpack --config config/webpack.config.prod.js --profile --json > webpack.stats.json",
		"lint": "eslint  --ext .js --ext .jsx app/scripts config test tools",
		"lint:styles": "stylelint 'app/styles/**/*.scss'",
		"test": "jest --config config/jest.config.js --bail --coverage",
		"test:watch": "jest --config config/jest.config.js --watch",
		"test:automation": "cypress run",
		"test:automation:start": "npm run start automation",
		"bundlesize": "bundlesize",
		"serve": "serve -o dist/",
		"deploy": "node tools deploy",
		"publish": "node tools publish",
		"precommit": "node tools upstream && npm run lint && npm run lint:styles && npm test",
		"postmerge": "node tools update"
	},
	"browserslist": [
		"last 2 versions",
		"safari >= 8",
		"ios >= 8"
